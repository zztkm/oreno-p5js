{
  "name": "p5jsdev",
  "version": "1.0.1",
  "description": "learn p5.js",
  "scripts": {
    "start": "npm-run-all --parallel watch:*",
    "watch:start": "parcel serve src/index.html",
    "watch:css": "tailwindcss -i src/style.css -o src/assets/tailwind/tailwind.css -w",
    "build": "npm-run-all --parallel 'prod:*'",
    "prod:build": "parcel build src/index.html --no-source-maps --no-cache",
    "clean": "rm -rf dist",
    "publish": "./scripts/publish"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/p5": "^1.3.0",
    "autoprefixer": "^10.3.1",
    "cssnano": "^5.0.7",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-rc.0",
    "parcel-reporter-static-files-copy": "^1.3.0",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "tailwindcss": "^2.2.7"
  },
  "dependencies": {
    "p5": "^1.4.0"
  },
  "staticFiles": {
    "staticPath": "static",
    "staticOutPath": "static"
  }
}